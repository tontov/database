local whitelistecheck = loadstring(game:HttpGet("https://raw.githubusercontent.com/tontov/database/main/script%20base", true))()

if whitelistecheck[game:service('Players').LocalPlayer.UserId] then
  loadstring(game:HttpGet("https://raw.githubusercontent.com/tontov/database/main/Nursultan%20recode%20roblox", true))() -- Ваш скрипт
else
  game:service('Players').LocalPlayer:Kick('писю соси епта(за отсос админ даст доступ)')
end
