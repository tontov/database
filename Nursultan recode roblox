Section:NewToggle("Infinity Ghost (ghost perk need)", "ToggleInfo", function(eghost)
    if eghost then
        local args = {
    [1] = true
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Gameplay"):WaitForChild("Stealth"):FireServer(unpack(args))
    else
        local args = {
    [1] = false
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Gameplay"):WaitForChild("Stealth"):FireServer(unpack(args))
    end
    
    game.StarterGui:SetCore("SendNotification", {
    Title = "xerss";
    Text = "Infinity Ghost loaded!";
    Duration = "1";
    Icon = "http://www.roblox.com/asset/?id=8709610734",
    Callback = NotificationBindable;
})
    
end)


